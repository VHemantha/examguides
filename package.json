{
  "name": "examguides",
  "version": "1.0.0",
  "description": "Multi-niche exam prep static web application",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build/build.js",
    "build:prod": "cross-env NODE_ENV=production node build/build.js",
    "dev": "node build/build.js --watch",
    "clean": "node -e \"import('fs').then(fs => fs.rmSync('dist', {recursive:true, force:true}))\"",
    "preview": "npx serve dist -p 3000 --no-clipboard"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.0"
  },
  "devDependencies": {
    "clean-css": "^5.3.3",
    "cross-env": "^7.0.3",
    "html-minifier-terser": "^7.2.0",
    "terser": "^5.37.0",
    "chokidar": "^3.6.0"
  }
}
